<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".MainActivity"
		android:id="@+id/test"
		tools:layout_editor_absoluteY="25dp"
		android:layout_margin="40dp"
		android:padding="40dp">

	<TextView
			android:layout_width="119dp"
			android:layout_height="41dp"
			android:text="@string/statusUnknow"
			android:id="@+id/networkLabel"
			tools:layout_editor_absoluteY="57dp"
			tools:layout_editor_absoluteX="102dp"/>

	<TextView
			android:text="down"
			android:layout_width="118dp"
			android:layout_height="36dp"
			android:id="@+id/powerGridLabel"
			tools:text="@string/statusUnknow"
			tools:layout_editor_absoluteY="123dp"
			app:layout_constraintStart_toStartOf="@+id/networkLabel"
			/>

	<TextView
			android:text="@string/network"
			android:layout_width="75dp"
			android:layout_height="21dp"
			tools:layout_editor_absoluteX="16dp"
			android:id="@+id/textView"
			app:layout_constraintTop_toTopOf="@+id/networkLabel"/>

	<TextView
			android:text="@string/powerGrid"
			android:layout_width="77dp"
			android:layout_height="18dp"
			android:id="@+id/textView2"
			app:layout_constraintTop_toTopOf="@+id/powerGridLabel"
			app:layout_constraintStart_toStartOf="@+id/textView"/>

</android.support.constraint.ConstraintLayout>